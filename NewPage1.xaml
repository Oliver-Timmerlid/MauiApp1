<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.NewPage1"
             Title="NewPage1"
             BackgroundColor="{DynamicResource OffBlack}">
    
    <VerticalStackLayout>
        <Label 
            Text="Welcome to SWEDEN!"
            TextColor="{DynamicResource Gray100}"
            VerticalOptions="Center" 
            HorizontalOptions="Center"
            Margin="0,50,0,10"/>
        <Button
                x:Name="ReturnBtn"
                Text="Tillbaka" 
                SemanticProperties.Hint="Return button to go to main page"
                Clicked="ReturnBtnClicked"
                HorizontalOptions="Center"
                BackgroundColor="{DynamicResource Gray100}"
                TextColor="{DynamicResource OffBlack}"
                Margin="0,50,0,10"/>
    </VerticalStackLayout>
</ContentPage>