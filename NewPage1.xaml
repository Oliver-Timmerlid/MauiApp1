<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MauiApp1.NewPage1"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="NewPage1"
    BackgroundColor="{DynamicResource OffBlack}">

    <VerticalStackLayout>
        <Label
            Margin="0,50,0,10"
            HorizontalOptions="Center"
            Text="Welcome to SWEDEN!"
            TextColor="{DynamicResource Gray100}"
            VerticalOptions="Center" />
        <Button
            x:Name="ReturnBtn"
            Margin="0,50,0,10"
            BackgroundColor="{DynamicResource Gray100}"
            Command="{Binding ReturnCommand}"
            HorizontalOptions="Center"
            SemanticProperties.Hint="Return button to go to main page"
            Text="Tillbaka"
            TextColor="{DynamicResource OffBlack}" />
    </VerticalStackLayout>
</ContentPage>